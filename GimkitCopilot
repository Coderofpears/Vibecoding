        "    files = [f for f in os.listdir(folder_path) if f.endswith(\".json\")]\n",
        "\n",
        "    if len(files) == 0:\n",
        "        print(f\"CRITICAL: No JSON files found in {folder_path}. Please upload files and restart.\")\n",
        "        return None\n",
        "\n",
        "    for filename in files:\n",
        "        with open(os.path.join(folder_path, filename), \"r\") as f:\n",
        "            try:\n",
        "                content = json.load(f)\n",
        "                combined_data.append({\n",
        "                    \"instruction\": \"Generate the game editor UI configuration JSON.\",\n",
        "                    \"input\": \"\",\n",
        "                    \"output\": json.dumps(content, indent=4)\n",
        "                })\n",
        "            except Exception as e:\n",
        "                print(f\"Skipping {filename} due to JSON error: {e}\")\n",
        "\n",
        "    return Dataset.from_list(combined_data)\n",
        "\n",
        "raw_dataset = load_json_folder(data_folder)\n",
        "\n",
        "# Stop execution if no data\n",
        "if raw_dataset is None:\n",
        "    raise ValueError(\"Dataset is empty. Upload your JSON files to /content/data/ first.\")\n",
        "\n",
        "# 4. PROMPT FORMATTING\n",
        "alpaca_prompt = \"\"\"### Instruction:\n",
        "{}\n",
        "\n",
        "### Input:\n",
        "{}\n",
        "\n",
        "### Response:\n",
        "{}\"\"\"\n",
        "\n",
        "def formatting_prompts_func(examples):\n",
        "    texts = [alpaca_prompt.format(inst, inp, out) + tokenizer.eos_token\n",
        "             for inst, inp, out in zip(examples[\"instruction\"], examples[\"input\"], examples[\"output\"])]\n",
        "    return { \"text\" : texts }\n",
        "\n",
        "dataset = raw_dataset.map(formatting_prompts_func, batched = True)\n",
        "\n",
        "# 5. CUSTOM 1-HOUR CHECKPOINT CALLBACK\n",
        "class HourlyCheckpoint(TrainerCallback):\n",
        "    def __init__(self, interval_hours=1):\n",
        "        self.interval = interval_hours * 3600\n",
        "        self.last_save = time.time()\n",
        "\n",
        "    def on_step_end(self, args, state, control, **kwargs):\n",
        "        if time.time() - self.last_save >= self.interval:\n",
        "            control.should_save = True\n",
        "            self.last_save = time.time()\n",
        "            print(f\"\\n[TIME CHECKPOINT] Saving at step {state.global_step}...\")\n",
        "        return control\n",
        "\n",
        "# 6. TRAINING SETUP (Fixed: Added tokenizer argument)\n",
        "trainer = SFTTrainer(\n",
        "    model = model,\n",
        "    tokenizer = tokenizer,              # <--- THIS WAS MISSING\n",
        "    train_dataset = dataset,\n",
        "    dataset_text_field = \"text\",\n",
        "    max_seq_length = max_seq_length,\n",
        "    callbacks = [HourlyCheckpoint(1)],\n",
        "    args = TrainingArguments(\n",
        "        per_device_train_batch_size = 1,\n",
        "        gradient_accumulation_steps = 1,\n",
        "        num_train_epochs = 20,\n",
        "        learning_rate = 2e-4,\n",
        "        fp16 = not torch.cuda.is_bf16_supported(),\n",
        "        bf16 = torch.cuda.is_bf16_supported(),\n",
        "        logging_steps = 1,\n",
        "        optim = \"paged_adamw_8bit\",\n",
        "        weight_decay = 0.01,\n",
        "        output_dir = \"outputs\",\n",
        "        save_strategy = \"steps\",\n",
        "        save_steps = 50,\n",
        "    ),\n",
        ")\n",
        "\n",
        "# 7. RUN TRAINING\n",
        "trainer.train()\n",
        "\n",
        "# 8. INFERENCE (TESTING)\n",
        "print(\"\\n--- TESTING THE MODEL ---\")\n",
        "FastLanguageModel.for_inference(model)\n",
        "inputs = tokenizer(\n",
        "    [alpaca_prompt.format(\"Generate the game editor UI configuration JSON.\", \"\", \"\")],\n",
        "    return_tensors = \"pt\"\n",
        ").to(\"cuda\")\n",
        "\n",
        "text_streamer = TextStreamer(tokenizer)\n",
        "_ = model.generate(**inputs, streamer = text_streamer, max_new_tokens = 1024)\n",
        "\n",
        "# 9. SAVE\n",
        "model.save_pretrained(\"lora_model\")\n",
        "tokenizer.save_pretrained(\"lora_model\")\n",
        "print(\"Saved to /content/lora_model\")"
      ]
    }
  ]
}